{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "parser": "@babel/eslint-parser",
    "rules": {
        "no-unused-vars": "warn",
        "react/prop-types": [2, { "skipUndeclared": true }],
        "react/display-name": "off",
        "react/no-children-prop": "off",
        "no-empty": "warn",
        "no-unreachable": "warn"
    }
    ,
    "ignorePatterns": ["webpack.config.js"]
}
